{
    "name": "zxin/socket-log-thinkphp",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "auooru"
        }
    ],
    "require": {
        "php": "^7.4 | ^8",
        "ext-curl": "*",
        "ext-json": "*",
        "topthink/framework": "^6.0.0 | ^8.0.0"
    },
    "require-dev": {
        "php-cs-fixer/shim": "^3.58"
    },
    "suggest": {
        "ext-openssl": "*",
        "ext-zlib": "*"
    },
    "autoload": {
        "psr-4": {
            "Zxin\\SocketLog\\": "src/",
            "think\\log\\driver\\": "think/"
        }
    },
    "config": {
        "sort-packages": true
    }
}
